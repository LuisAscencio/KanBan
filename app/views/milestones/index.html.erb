
 <h1>Welcome <%= current_user.name.capitalize %></h1> 

 <%= link_to  "logout", logout_path %> 
 <br>

<%= button_to  "Set a New Milestone", new_milestone_path, :action => 'get', :method => "get" %>   

<% @milestones.each do |ms| %>
<% if ms.user_id == current_user.id %>
    <h3><a href="/milestones/<%= ms.id %>"><%= ms.title %></a></h3>
    <% end %>   
<% end %>   