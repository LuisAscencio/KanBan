<h1>L O G I N</h1>

<% if flash[:errors] %>
<% flash[:errors].each do |error| %>
<p><%= error %></p>
<% end %>
<% end %>

<%= form_tag(sessions_path) do %>

<%= text_field_tag :username, "Username"%>

<%= text_field_tag :password, "Password"%>

<%= submit_tag :submit %>
<% end %>

<br>
<%= link_to  "or Sign Up", new_user_path %>
